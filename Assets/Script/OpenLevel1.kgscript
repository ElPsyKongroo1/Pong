void OpenLevel1()
{
	UI::LoadUI("UserInterface/RuntimeUI.kgui");
	UI::DisplayWindow("pre_game_level_warning", true);
	UI::DisplayWindow("base_window", false);
	Scenes::LoadScene("Scenes/level_1.kgscene");
	Input::LoadInputMap("Input/Pre_Start.kginput");
	Audio::PlaySound("Audio/menu_confirm.kgaudio");
	Audio::PlayMusic("Audio/green_a.kgaudio");
}
