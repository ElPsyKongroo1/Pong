void OpenMainMenu()
{
	UIService::LoadUI("Common/UI/Main Menu.kgui");
	SceneService::LoadScene("Scenes/MainMenu/main_menu.kgscene");
	InputService::LoadInputMap("Common/Input/MainMenu.kginput");
	AudioService::PlayMusic("Common/Audio/blue_a.kgaudio");
	GameStateService::SetField("GameMode", 0);
	NetworkService::RequestUserCount();
}